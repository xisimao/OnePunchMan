{
  "Logging": {
    "HoldDays": 7,
    "Directory": "logs",
    "Default": "Information",
    "Logger": [
      {
        "Type": "Console",
        "MinLevel": "Debug",
        "MaxLevel": "Error"
      },
      {
        "Type": "File",
        "Name": "Traffic",
        "MinLevel": "Information",
        "MaxLevel": "Error"
      }
    ]
  },
  //流量系统
  "Flow": {
    //计算两车是否处于排队状态的最小距离，单位：像素
    "QueueMinDistance": 200
  },
  //事件系统
  "Event": {
    //开始计算停车事件的时间长度，单位:秒
    "ParkStartSpan": 60,
    //确认停车事件的时间长度，单位:秒
    "ParkEndSpan": 120,
    //判断拥堵时，区域内的最小机动车数量
    "CongestionCarCount": 4,
    //上报拥堵的时间间隔,单位:秒
    "CongestionReportSpan": 60,
    //判断车辆逆行点的最小逆行距离，单位：像素
    "RetrogradeMinMove": 50,
    //确定车辆逆行的最小逆行点的个数
    "RetrogradeMinCount": 3,
    //输出视频的时长，单位：秒
    "OutputVideoSpan": 10
  }
}